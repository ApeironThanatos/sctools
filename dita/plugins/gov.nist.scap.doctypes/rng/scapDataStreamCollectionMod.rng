  <div>
    <a:documentation>COMMON ATTRIBUTE SETS</a:documentation>
    <define name="chapter-atts">
      <optional>
        <attribute name="navtitle"/>
      </optional>
      <optional>
        <attribute name="href"/>
      </optional>
      <optional>
        <attribute name="keyref"/>
      </optional>
      <optional>
        <attribute name="keys"/>
      </optional>
      <optional>
        <attribute name="copy-to"/>
      </optional>
      <optional>
        <attribute name="outputclass"/>
      </optional>
      <ref name="topicref-atts"/>
      <ref name="univ-atts"/>
    </define>

  </div>
  <div>
    <a:documentation>ELEMENT TYPE PATTERNS</a:documentation>

    <div>
      <a:documentation>LONG NAME: SCAP Source Data Stream Collection</a:documentation>
      <define name="scapSourceDataStreamCollection.content">
        <optional>
          <choice>
            <ref name="title"/>
            <ref name="booktitle"/>
          </choice>
        </optional>
        <optional>
          <ref name="bookmeta"/>
        </optional>
        <optional>
          <ref name="frontmatter"/>
        </optional>
        <zeroOrMore>
          <ref name="chapter"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="part"/>
        </zeroOrMore>
        <choice>
          <optional>
            <ref name="appendices"/>
          </optional>
          <zeroOrMore>
            <ref name="appendix"/>
          </zeroOrMore>
        </choice>
        <optional>
          <ref name="backmatter"/>
        </optional>
        <zeroOrMore>
          <ref name="reltable"/>
        </zeroOrMore>
      </define>
      <define name="scapDataStreamCollection.attributes">
        <optional>
          <attribute name="id">
            <data type="ID"/>
          </attribute>
        </optional>
        <ref name="conref-atts"/>
        <optional>
          <attribute name="anchorref"/>
        </optional>
        <optional>
          <attribute name="outputclass"/>
        </optional>
        <ref name="localization-atts"/>
        <ref name="topicref-atts"/>
        <ref name="select-atts"/>
      </define>
      <define name="scapDataStreamCollection.element">
        <element name="scapDataStreamCollection" dita:longName="Book Map">
          <a:documentation>The &lt;bookmap> element is a map file used to organize DITA content into a traditional book format. Category: Bookmap elements</a:documentation>
          <ref name="bookmap.attlist"/>
          <ref name="bookmap.content"/>
        </element>
      </define>
      <define name="bookmap.attlist" combine="interleave">
        <ref name="bookmap.attributes"/>
        <ref name="arch-atts"/>
        <ref name="domains-att"/>
      </define>

    </div>
  </div>
  <div>
    <a:documentation>SPECIALIZATION ATTRIBUTES</a:documentation>

    <define name="scapDataStreamCollection.attlist" combine="interleave">
      <ref name="global-atts"/>
      <optional>
        <attribute name="class" a:defaultValue="- map/map scapDataStreamCollection/scapDataStreamCollection "/>
      </optional>
    </define>
  </div>
